<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.luciano.testesantigo.Intent.IntentComResultado.ActivityPrincipalTelas.APTela2"
    android:orientation="vertical">

    <EditText
        android:id="@+id/campoTexto1"
        android:hint="Your fist name"
        android:singleLine="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <EditText
        android:id="@+id/campoTexto2"
        android:hint="Your last name"
        android:singleLine="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:text="SEND"
        android:textColor="#ff000000"
        android:onClick="enviarOK1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>
